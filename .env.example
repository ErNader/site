# OpenRouter API Key - get from https://openrouter.ai
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Flask app secret key (for session encryption)
FLASK_SECRET_KEY=generate-a-secure-random-string-here

# GitHub OAuth settings - get from https://github.com/settings/developers
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Google OAuth settings - get from https://console.cloud.google.com
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Database settings (SQLite by default)
SQLALCHEMY_DATABASE_URI=sqlite:///chat.db 